	quest.7EF7546FF007EF36.title: "&2Биотопливо"
	quest.7EFFA8FF1CAD80E3.quest_subtitle: "Взорвите чёрный кварц и катализатор"
	quest.7EFFA8FF1CAD80E3.title: "&fРастущая жеода чёрного кварца"
	quest.7F141E9CF9676D0F.title: "&6Предсказание Визера"
	quest.7F1B3EDA4DAFF0CB.quest_subtitle: "Полив 9x9!"
	quest.7F1B3EDA4DAFF0CB.title: "&bИмперская &fлейка"
	quest.7F1D6971036A31DA.quest_desc: [
