	quest.6E4F3FE32C5D8285.quest_desc: ["&7&oСлучайным образом получить улучшение Bene."]
	quest.6E4F3FE32C5D8285.title: "&bСумка с улучшениями для пчел"
	quest.6E62CF3784A15E0F.title: "&fХимическая карта"
	quest.6E66D752E153E00F.title: "&fПолезные &aУровень 1&f Глифы"
	quest.6E7E4899C5468DED.quest_desc: [
